<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.refinement.mapper.DepartmentMapper">

    <select id="listDeptIds" resultType="java.lang.String">
        SELECT
            deptid
        FROM
            department
        ORDER BY
            create_time DESC
    </select>
</mapper>